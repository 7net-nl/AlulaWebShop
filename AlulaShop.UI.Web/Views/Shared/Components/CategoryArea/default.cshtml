@model CategoryDto

<div class="row p-5 m-5">
    <div class="container">
        <div class="col-lg-12">
            <div class="text-danger">
                <h1>Category Name (@Model.Name)</h1>
            </div>
            <span class="badge badge-pill badge-primary">@Model.Name</span>
            <span class="badge badge-pill badge-secondary">@Model.Name</span>
            <span class="badge badge-pill badge-success">@Model.Name</span>
            <span class="badge badge-pill badge-danger">@Model.Name</span>
            <span class="badge badge-pill badge-warning">@Model.Name</span>
            <span class="badge badge-pill badge-info">@Model.Name</span>
            <span class="badge badge-pill badge-light">@Model.Name</span>
            <span class="badge badge-pill badge-dark">@Model.Name</span>
        </div>
    </div>
</div>